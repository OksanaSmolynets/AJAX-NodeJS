<!DOCTYPE html>
<html lang="en">
<style>
  ul {
    list-style-type: none;
  }
</style>

<body class="container">
  <main>
    <ul>
      <% users.forEach(function(user) { %>
        <li>
          <h2>
            User id: <%= user.id %>
          </h2>
          <p>
            Name: <%= user.first_name %>
          </p>
          <p>
            Last name: <%= user.last_name %>
          </p>
          <p>
            Email: <%= user.email %>
          </p>
          <img src=<%=user.photo %>>
        </li>
        <% }); %>
    </ul>
  </main>
</body>


</html>